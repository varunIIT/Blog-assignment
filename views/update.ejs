<div id="update">
    <h2 style="margin-top: 0;">Update a Blog</h2>
    <form action="/update/<%=id%>" method="POST">
        <input type="text" name="title" placeholder="title..."><br>
        <textarea name="body" cols="18" rows="8" placeholder="body..."></textarea><br>
        <button type="submit">Update</button>
    </form>
</div>